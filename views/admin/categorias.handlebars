<h2>Lista de Categorias</h2>
<hr>
<a href="/admin/categorias/add">
    <button type="button" class="btn btn-success">Nova categoria</button>
</a>

{{#each categorias}}
    <div class="card">
        <div class="card-body">
            <h4>{{nome}}</h4>
            <small>{{slug}}</small>
        </div>
    </div>
{{else}}

{{/each}}